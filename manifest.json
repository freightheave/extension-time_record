{
    "name": "time tracker",
    "author": "Roshan Komaravolu",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Timeout webpage afer specified amount of time for the day.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "src.js"
      },
    "host_permissions": [
        "http://www.twitch.tv/*",
        "https://www.twitch.tv/*",
        "https://www.reddit.com/*",
        "http://www.reddit.com/*"
      ],
    "icons": {
        "128": "icons/icon128x128.png"
    } 
}